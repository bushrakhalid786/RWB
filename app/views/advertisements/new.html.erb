<%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<% if @category.alias_name == "auto-accessories-parts" %>
  <%= render 'auto_accessories_form', advertisement: @advertisement %>
<% elsif @category.alias_name == "user-car-for-sale" %>
  <%= render 'used_car_for_sale_form', advertisement: @advertisement %>  
<% else %>
  <%= render 'form', advertisement: @advertisement %>
<% end %>